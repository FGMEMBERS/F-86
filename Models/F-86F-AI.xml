  <nasal>
    <load><![CDATA[
			var self = cmdarg();

			# Define the properties for Livery and Variant
			var livery = self.getNode("sim/model/livery",1);
			livery.setValue("l3000-1b.png");
			var pilot_node = self.getNode("sim/model/crew/pilot");
			io.read_properties("Aircraft/F-86/Models/Human/fighterpilot50_pose.xml", pilot_node);


			# This is the same for all Models

			var combat = self.getNode("combat/combat",1);
			combat.setValue("true");

			var destroyed = self.getNode("combat/destroyed",1);
			destroyed.setBoolValue(0);
			var explosion = self.getNode("combat/explosion",1);
			explosion.setBoolValue(0);
			var damage = self.getNode("combat/damage",1);
			damage.setValue(0);

			var hitcount = self.getNode("hitcount",1);
			var size = self.getNode("size",1);
			var lat = self.getNode("position/latitude-deg",1);
			var lon = self.getNode("position/longitude-deg",1);       
			var lm = self.getNode("controls/flight/lateral-mode",1);
			var speed = self.getNode("controls/flight/target-spd",1);
			var head = self.getNode("controls/flight/target-hdg",1);
			var alt = self.getNode("position/altitude-ft",1);
			var roam = self.getNode("controls/roam-radius-deg",1);
			var pitch = self.getNode("orientation/pitch-deg",1);
			var roll = self.getNode("orientation/roll-deg",1);
			lm.setValue("yaw");
			hitcount.setValue(0);
			size.setValue(20);
			roam.setValue(0.02);
			var rlat = lat.getValue();    
			var rlon = lon.getValue();
			var v0 = 0;
			


			var loopid = 0;
			var loop = func(id) {
				id == loopid or return;
				movement();
				hitcheck();



				settimer(func { loop(id) }, 0);
			}
			settimer(func { loop(loopid) }, 0.5);

		###	self steering stuff
		var movement = func() {
				var la = self.getNode("position/latitude-deg",1).getValue();
				var lo = self.getNode("position/longitude-deg",1).getValue();	
				var alt0 = self.getNode("position/altitude-ft",1).getValue();
			#	var calt0 = geo.elevation(la+0.0000001,lo-0.0000001);
			#	var calt1 = geo.elevation(la-0.0000001,lo+0.0000001);
				
			#	var finalt = (calt0 + calt1 )/2;

			#	alt.setValue(finalt * M2FT);
				#self.getNode("orientation/model-pitch",1).setValue ((alt0-finalt*M2FT)*100);
				#print ((alt0-finalt*M2FT)*100);
				var currhead = self.getNode("controls/flight/target-hdg",1).getValue();
				var co1 = abs(la -rlat);
				var ro1 = self.getNode("controls/roam-radius-deg",1).getValue();
#				print (co1);
				if (co1 > ro1) {
#					print ("out of bounds");
					if ( currhead > 347 ) {   
						self.getNode("controls/flight/target-hdg",1).setValue(4*rand());
					} else {
						self.getNode("controls/flight/target-hdg",1).setValue(currhead + 4*rand());
					}
				}
		}

			#	hitcheck stuff
		var hitcheck = func() {
				var v = hitcount.getValue();
			#print ("Hitcount: ",hitcount.getValue());			# debug output 
				if ( v > v0 )	{

							

							
					v0=v;
									
				}

				if (v != nil and v > 9) {	# 
					print ("Ouch!");					# debug output

					speed.setValue( 0 );
					destroyed.setValue(1);
					explosion.setBoolValue(1);
					settimer( func { explosion.setBoolValue(0) },3);
					loopid += 1;							# end loop
				}
		}

      ]]></load>
    <unload><![CDATA[
			print ("bye");
			loopid += 1;
      ]]></unload>
