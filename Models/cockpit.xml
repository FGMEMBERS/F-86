      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0.05</z>
      </axis>
    </animation>

<!-- pick -->
 <animation>
  <type>rotate</type>
    <object-name>gearhandle.knob</object-name>
    <object-name>gearhandle.knoblight</object-name>
    <object-name>gearhandle.lever</object-name>
  <property>controls/gear/gear-down</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>-20</dep>
   </entry>
  <entry>
    <ind>1.00</ind>
    <dep>20</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>1.93441</x-m>
   <y-m>0</y-m>
   <z-m>0.25376</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
    <type>pick</type>
    <object-name>gearhandle.knob</object-name>
    <object-name>gearhandle.knoblight</object-name>
    <object-name>gearhandle.lever</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/gear/gear-down</property>

      </binding>
    </action>
 </animation>

  <animation>
    <type>pick</type>
    <object-name>hydpresssel.up</object-name>
    <object-name>hydpresssel.neutral</object-name>
    <object-name>hydpresssel.down</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/hydpressel</property>

      </binding>
    </action>
 </animation>

