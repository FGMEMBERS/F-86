<?xml version="1.0"?>

<PropertyList>
	
	<!--back-->
	<!--right -->
	<!--up -->
	

     <model>
		<name>left Droptank</name>
		<path>Aircraft/F-86/Models/droptank.ac</path>
		<offsets>
