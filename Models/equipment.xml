			<z-m>-1.2</z-m>
			<pitch-deg>0.0</pitch-deg>
		</offsets>
	</model>	

     <model>
		<name>left droptank pylon</name>
		<path>Aircraft/F-86/Models/pylon.ac</path>
		<offsets>
			<x-m>4.7</x-m>
			<y-m>3.0</y-m>
			<z-m>-1.2</z-m>
			<pitch-deg>0.0</pitch-deg>
		</offsets>
	</model>	

     <model>
		<name>right Droptank</name>
		<path>Aircraft/F-86/Models/droptank.ac</path>
		<offsets>
			<x-m>4.7</x-m>
			<y-m>-3.0</y-m>
			<z-m>-1.2</z-m>
			<pitch-deg>0.0</pitch-deg>
		</offsets>
	</model>	

     <model>
		<name>right droptank pylon</name>
		<path>Aircraft/F-86/Models/pylon.ac</path>
		<offsets>
			<x-m>4.7</x-m>
			<y-m>-3.0</y-m>
