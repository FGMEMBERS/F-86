	<!--+++++++++++++++++++++++++++++++++++++++++++++-->
	<!--++++++++++++  ANIMATIONS  +++++++++++++++++++-->
	<!--+++++++++++++++++++++++++++++++++++++++++++++-->



<!--Droptank -->

 <animation>
  <type>select</type>
  <object-name>left Droptank</object-name>
  <condition>
   <greater-than>
    <property>sim/weight[0]/weight-lb</property>
    <value>149.0</value>
   </greater-than>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>right Droptank</object-name>
  <condition>
   <greater-than>
    <property>sim/weight[1]/weight-lb</property>
    <value>149.0</value>
   </greater-than>
  </condition>
 </animation>




</PropertyList>
