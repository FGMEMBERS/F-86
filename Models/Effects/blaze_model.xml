<?xml version="1.0"?>
<PropertyList>
	<path>blaze.ac</path>
	
	 <animation>
   <type>select</type>

     <condition>
       <equals>
         <property>sim/model/mosquito/lighting/flash-l/state</property>
         <value>true</value>
       </equals>
     </condition>
 </animation>
	 <animation>
  <type>billboard</type>
  <spherical>false</spherical>
 </animation>
	 <animation>
   <type>material</type>
     <emission>
       <red>1</red>
       <green>1</green>
       <blue>1</blue>
     </emission>
 </animation>
	
</PropertyList>
