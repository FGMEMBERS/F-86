<?xml version="1.0"?>
<PropertyList>

 <path>cabinpress.ac</path>

 <animation>
  <type>material</type>
  <object-name>cabinpress</object-name>
  <object-name>cabinneedle</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
