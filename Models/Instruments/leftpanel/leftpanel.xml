<?xml version="1.0"?>
<PropertyList>

 <path>leftpanel.ac</path>

 <animation>
  <type>select</type>
  <object-name>nosewheelup</object-name>
  <condition>
   <equals>
    <property>gear/gear[0]/position-norm</property>
    <value>0.0</value>
   </equals>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>leftwheelup</object-name>
  <condition>
   <equals>
    <property>gear/gear[1]/position-norm</property>
    <value>0.0</value>
   </equals>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>rightwheelup</object-name>
  <condition>
   <equals>
    <property>gear/gear[2]/position-norm</property>
    <value>0.0</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>nosewheeldown</object-name>
  <condition>
   <equals>
    <property>gear/gear[0]/position-norm</property>
    <value>1.0</value>
   </equals>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>leftwheeldown</object-name>
  <condition>
   <equals>
    <property>gear/gear[1]/position-norm</property>
    <value>1.0</value>
   </equals>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>rightwheeldown</object-name>
  <condition>
   <equals>
    <property>gear/gear[2]/position-norm</property>
    <value>1.0</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>nosewheelunlocked</object-name>
  <condition>
   <and>
   <not-equals>
    <property>gear/gear[0]/position-norm</property>
    <value>0.0</value>
   </not-equals>
   <not-equals>
    <property>gear/gear[0]/position-norm</property>
    <value>1.0</value>
   </not-equals>

   </and>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>leftwheelunlocked</object-name>
  <condition>
   <and>
   <not-equals>
    <property>gear/gear[1]/position-norm</property>
    <value>0.0</value>
   </not-equals>
   <not-equals>
    <property>gear/gear[1]/position-norm</property>
    <value>1.0</value>
   </not-equals>

   </and>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>rightwheelunlocked</object-name>
  <condition>
   <and>
   <not-equals>
    <property>gear/gear[2]/position-norm</property>
    <value>0.0</value>
   </not-equals>
   <not-equals>
    <property>gear/gear[2]/position-norm</property>
    <value>1.0</value>
   </not-equals>

   </and>
  </condition>
 </animation>

  <animation>
    <type>pick</type>
    <object-name>landtaxilight.on</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/light/landtaxi</property>
        <value>true</value>
      </binding>
    </action>
 </animation>
  <animation>
    <type>pick</type>
    <object-name>landtaxilight.off</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/light/landtaxi</property>
        <value>false</value>
      </binding>
    </action>
 </animation>

  <animation>
    <type>pick</type>
    <object-name>canopy.open</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/canopy/position-norm</property>
        <step>0.01</step>
        <min>0</min>
        <max>1</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>
  <animation>
    <type>pick</type>
    <object-name>canopy.close</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/canopy/position-norm</property>
        <step>-0.01</step>
        <min>0</min>
        <max>1</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>
</PropertyList>


