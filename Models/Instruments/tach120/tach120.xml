<?xml version="1.0"?>
<PropertyList>

 <path>tach120.ac</path>

 <animation>
  <type>material</type>
  <object-name>tach120</object-name>
  <object-name>needle1</object-name>
  <object-name>needle2</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>3</max-m>
 </animation>


 <animation>
  <type>rotate</type>
  <object-name>needle2</object-name>
  <property>engines/engine[0]/n2</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-83</dep>
   </entry>
   <entry>
    <ind>30</ind>
    <dep>-252</dep>
   </entry>
   <entry>
    <ind>50</ind>
    <dep>-360</dep>
   </entry>
  </interpolation>
  <axis>

   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle1</object-name>
  <property>engines/engine[0]/n2</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>50</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>70</ind>
    <dep>-114</dep>
   </entry>
   <entry>
    <ind>90</ind>
    <dep>-220</dep>
   </entry>
   <entry>
    <ind>110</ind>
    <dep>-333</dep>
   </entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
</PropertyList>


