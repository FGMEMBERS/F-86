<?xml version="1.0"?>
<PropertyList>

 <path>fuelflow.ac</path>

 <animation>
  <type>material</type>
  <object-name>fuelflow</object-name>
  <object-name>needle1</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>3</max-m>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle1</object-name>
  <property>engines/engine[0]/fuel-flow-gph</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>45</dep>
   </entry>
   <entry>
    <ind>605</ind>
    <dep>-227</dep>
   </entry>
   <entry>
    <ind>1200</ind>
    <dep>-295</dep>
   </entry>
  </interpolation>
  <axis>

   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
</PropertyList>


