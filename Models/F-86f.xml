<!-- nosegear -->   
    <animation> 
      <type>rotate</type>
      <object-name>nosegear1</object-name>
      <object-name>nosewheel</object-name>
      <property>gear/gear[0]/position-norm</property>
      <interpolation> 
        <entry>
          <ind>0</ind>
          <dep>100</dep>
        </entry>
        <entry>
          <ind>0.15</ind>
          <dep>100</dep>
        </entry>
        <entry>
          <ind>0.85</ind>
          <dep>0</dep>
        </entry>
        <entry>
          <ind>1</ind>
          <dep>0</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0.911</x-m>
        <y-m>0</y-m>
        <z-m>-0.74</z-m>
      </center>
      <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>

